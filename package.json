{
  "name": "vue-jsonschema-form",
  "version": "1.0.0",
  "description": "A simple Vue component capable of building HTML forms out of a JSON schema",
  "main": "dist/vue-jsonschema-form.umd.js",
  "module": "dist/vue-jsonschema-form.esm.js",
  "unpkg": "dist/vue-jsonschema-form.min.js",
  "scripts": {
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "prepublishOnly": "npm run test && npm run build",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch",
    "test": "npm run test:unit",
    "test:unit": "jest --config tests/unit/jest.config.js --coverage",
    "example": "webpack --config examples/webpack.config.js --colors --progress --watch",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "docs": "cd docs && gitbook serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okbeng03/vue-form.git"
  },
  "keywords": [
    "vue",
    "form",
    "json schema",
    "vuex"
  ],
  "author": "changbin.wangcb",
  "license": "BSD-2-Clause",
  "readmeFilename": "README.md",
  "devDependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "bootstrap": "~3.3.6",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.3.0",
    "jest-vue": "^0.8.2",
    "less": "^2.5.3",
    "less-loader": "~2.2.2",
    "lodash": "~4.15.0",
    "moment": "~2.11.1",
    "rollup": "^0.51.8",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^2.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.4.1",
    "style-loader": "~0.13.0",
    "uglify-es": "^3.3.9",
    "unicode-loader": "~1.0.6",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "vuex": "^3.1.0",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^1.1.2",
    "ajv": "^6.5.3",
    "ajv-errors": "^1.0.1",
    "extend": "^3.0.2",
    "objectpath": "^1.2.2",
    "vue-picture-input": "^2.1.6",
    "vue-select": "^2.5.1",
    "vue2-datepicker": "^2.8.1",
    "vuedraggable": "^2.17.0"
  }
}
